{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "devCommand": "npm run serve",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "*.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ]
}